{
  "name": "yunyoujun-sponsors",
  "version": "2.0.0",
  "private": true,
  "description": "YunYouJun's Sponsors | 云游君的赞助者们",
  "license": "MIT",
  "author": "YunYouJun <me@yunyoujun.cn>",
  "repository": {
    "type": "git",
    "url": "https://github.com/YunYouJun/sponsors"
  },
  "scripts": {
    "dev": "pnpm run dev -C packages/site",
    "build": "pnpm run build -C packages/site",
    "build:json": "pnpm run build -C packages/cli && cp packages/cli/dist/* packages/site/dist/",
    "generate": "pnpm run generate -C kit",
    "generate:force": "pnpm run generate:force -C kit",
    "generate:wide": "pnpm run generate:wide -C kit",
    "lint": "eslint . --fix",
    "update": "git add -A && git commit -m 'chore: add new sponsor' && git push",
    "sponsors": "pnpm run add -C packages/cli"
  },
  "bugs": {
    "url": "https://github.com/YunYouJun/sponsors/issues"
  },
  "homepage": "https://sponsors.yunyoujun.cn",
  "dependencies": {
    "@yunyoujun/logger": "^0.2.1",
    "commander": "^9.1.0",
    "dayjs": "^1.11.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.19.4",
    "@types/js-yaml": "^4.0.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.12.0",
    "esno": "^0.14.1",
    "typescript": "^4.6.3"
  }
}
